import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt
# 1. Load the data (assuming 'cars2018.csv' is in the current directory)
df_cars2018 = pd.read_csv('cars2018.csv')
# 2. Create the scatter plot using seaborn
plt.figure(figsize=(10, 6)) # Optional: set plot size
sns.scatterplot(
data=df_cars2018,
x='Displacement',         # Maps Displacement to the x-axis
y='MPG',                  # Maps MPG to the y-axis
hue='Transmission',       # Maps Transmission to the color aesthetic (like R's color=)
)
plt.title('MPG vs. Displacement, Colored by Transmission')
plt.grid(True)
plt.show() # In a Python environment, this displays the plot
